<h2>Edit </h2>

<%= form_for(@user, url: person_path(@user), html: { method: :put }) do |f| %>



<%=f.fields_for :userinfo_attributes do |u|%>
<%= u.label :address %>
 <%= u.text_field :address %>
    <%= u.label :city %>
    <%= u.text_field :city %>
    <%= u.label :state %>
    <%= u.text_field :state %>
    <%= u.label :country %>
    <%= u.text_field :country %>
    <%= u.label :birthday %>
    <%= u.text_field :bday %>
    <%= u.label :full_name %>
    <%= u.text_field :full_name %>
    <%= u.label :zipcode %>
    <%= u.text_field :zip %>
    <%= u.label :login %>
   <%= u.text_field :login %>

   <%end%>
  <div><%= f.submit "Update" %></div>
<% end %>



<%= link_to "Back", :back %>